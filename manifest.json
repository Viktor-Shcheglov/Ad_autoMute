{
    "name":"obj ext",
    "description":"my ext",
    "version": "0.0.1",
    "manifest_version":2,
    "icons":{
        "16": "./32px.png"
    },
    "background":{
        "scripts":["./background.js"],
        "persistent": true
    },
    "browser_action":{
        "default_popup": "popup.html"
    },
    "permissions":[
        "tabs",
        "https://www.twitch.tv/*",
        "*://*.twitch.tv/*",
        "*://*.ttvnw.net/*",
        "http://*/",
        "https://*/",
		"webRequest",
		"webRequestBlocking"
    ]
}